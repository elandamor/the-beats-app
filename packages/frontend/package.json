{
  "name": "frontend",
  "version": "0.0.1",
  "description": "Frontend for the-beats-app",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elandamor/the-beats-app.git"
  },
  "keywords": [
    "music",
    "streaming",
    "pwa"
  ],
  "author": "Thandolwethu Mpofu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elandamor/the-beats-app/issues"
  },
  "homepage": "https://github.com/elandamor/the-beats-app#readme",
  "scripts": {
    "build": "the-react-scripts build",
    "cy:open": "cypress open",
    "generate": "the-react-scripts generate",
    "generate:schema": "apollo schema:download --endpoint http://localhost:4000 schema.json",
    "generate:schemaTypes": "apollo codegen:generate --localSchemaFile=schema.json --target=typescript --globalTypesFile=src/@types/graphql-global-types.ts types",
    "start": "cross-env NODE_ENV=development the-react-scripts start",
    "start:prod": "cross-env NODE_ENV=production the-react-scripts start",
    "test": "jest --watch"
  },
  "dependencies": {
    "@apollo/react-components": "3.1.2",
    "@apollo/react-hooks": "3.1.2",
    "@apollo/react-testing": "3.1.2",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-cache-persist": "0.1.1",
    "apollo-client": "2.6.4",
    "apollo-link": "1.2.13",
    "apollo-link-error": "1.1.12",
    "apollo-link-http": "1.5.16",
    "apollo-link-retry": "2.2.15",
    "classnames": "2.2.6",
    "formik": "1.5.8",
    "graphql": "14.5.8",
    "howler": "2.1.2",
    "moment": "2.24.0",
    "moment-range": "4.0.2",
    "no-scroll": "2.1.1",
    "react": "16.10.2",
    "react-dates": "21.2.0",
    "react-dom": "16.10.2",
    "react-dropzone": "10.1.10",
    "react-feather": "2.0.3",
    "react-helmet": "5.2.1",
    "react-lottie": "1.2.3",
    "react-measure": "2.3.0",
    "react-router-dom": "5.1.2",
    "react-select": "3.0.8",
    "react-spring": "8.0.27",
    "react-table": "6.10.3",
    "styled-components": "4.4.0",
    "styled-normalize": "8.0.6",
    "styled-system": "5.1.2",
    "typeface-roboto": "0.0.75",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@types/howler": "2.1.1",
    "@types/react-dates": "17.1.5",
    "@types/styled-system": "5.1.2",
    "@types/yup": "0.26.24",
    "apollo": "2.19.0",
    "babel-loader": "8.0.6",
    "cypress": "3.4.1",
    "the-react-scripts": "0.1.1",
    "why-did-you-update": "1.0.6"
  }
}
