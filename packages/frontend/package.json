    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-retry": "^2.2.15",
    "classnames": "^2.2.6",
    "d3": "^5.12.0",
    "formik": "^1.5.8",
    "graphql": "^14.5.4",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "no-scroll": "^2.1.1",
