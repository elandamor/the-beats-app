  "scripts": {
    "build": "the-react-scripts build",
    "generate": "the-react-scripts generate",
    "generate:schema": "apollo schema:download --endpoint http://localhost:4000 schema.json",
    "generate:schemaTypes": "apollo codegen:generate --localSchemaFile=schema.json --target=typescript --globalTypesFile=src/@types/graphql-global-types.ts types",
    "start": "cross-env NODE_ENV=development the-react-scripts start",
    "start:prod": "cross-env NODE_ENV=production the-react-scripts start",
    "test": "jest --watch"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.0",
    "@apollo/react-testing": "^3.1.0",
    "apexcharts": "^3.8.6",
