{
  "name": "frontend",
  "version": "0.0.1",
  "description": "Frontend for the-beats-app",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elandamor/the-beats-app.git"
  },
  "keywords": [
    "music",
    "streaming",
    "pwa"
  ],
  "author": "Thandolwethu Mpofu",
