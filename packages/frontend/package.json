    "react-spring": "^8.0.27",
    "react-table": "^6.10.3",
    "react-vis": "^1.11.7",
    "styled-components": "^4.3.2",
    "styled-normalize": "^8.0.6",
    "styled-system": "^5.1.2",
    "typeface-roboto": "^0.0.75",
    "yup": "^0.27.0"
